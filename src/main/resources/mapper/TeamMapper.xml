<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.peermatch.mapper.TeamMapper">
    <resultMap id="BaseResultMap" type="org.example.peermatch.model.domain.Team">
            <id property="id" column="id" />
            <result property="teamName" column="team_name" />
            <result property="description" column="description" />
            <result property="userId" column="user_id" />
            <result property="maxNum" column="max_num" />
            <result property="expireTime" column="expire_time" />
            <result property="teamStatus" column="team_status" />
            <result property="password" column="password" />
            <result property="isDelete" column="is_delete" />
            <result property="createTime" column="create_time" />
            <result property="updateTime" column="update_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,team_name,description,user_id,max_num,expire_time,
        team_status,password,is_delete,create_time,update_time
    </sql>
</mapper>
